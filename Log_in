import java.awt.Robot;
import java.awt.event.InputEvent;

public class Log_in {
	
	public void log_in(String a,String b) {
		ClipboardCP cm=new ClipboardCP();
		
		cm.copy("https://nid.naver.com/nidlogin.login?mode=form&url=https%3A%2F%2Fwww.naver.com");
		
		try {
			Robot r=new Robot();			
			
			r.delay(300);
			r.mouseMove(273,56);
			r.mousePress(InputEvent.BUTTON1_DOWN_MASK);
			r.mouseRelease(InputEvent.BUTTON1_DOWN_MASK);			
			
			r.keyPress(17);
			r.keyPress(86);
			r.keyRelease(86);
			r.keyRelease(17);
			
			r.delay(300);
			r.keyPress(10);
			r.keyRelease(10);
			r.delay(300);
			
		}
		catch(Exception e){
			e.printStackTrace();
		}
		
		cm.copy(a);		
		
		try {
			Robot r=new Robot();			
			
			r.mouseMove(1133,412);
			r.mousePress(InputEvent.BUTTON1_DOWN_MASK);
			r.mouseRelease(InputEvent.BUTTON1_DOWN_MASK);			
			
			r.keyPress(17);
			r.keyPress(86);
			r.keyRelease(86);
			r.keyRelease(17);
			r.delay(500);
		}
		catch(Exception e){
			e.printStackTrace();
		}
		
		cm.copy(b);
		
		try {
			Robot r=new Robot();			
			r.keyPress(9);
			r.keyRelease(9);
			
			r.delay(300);
			r.keyPress(17);
			r.keyPress(86);
			r.keyRelease(86);
			r.keyRelease(17);
			
			r.delay(300);
			r.keyPress(10);
			r.keyRelease(10);
		}
		catch(Exception e){
			e.printStackTrace();
		}
	}
	
	
	public void cafe_inH() {
		ClipboardCP cm=new ClipboardCP();
		cm.copy("https://cafe.naver.com/healim0071");
		
		try {
			Robot r=new Robot();
			r.delay(300);
			r.mouseMove(268,18);
			r.mousePress(InputEvent.BUTTON1_DOWN_MASK);
			r.mouseRelease(InputEvent.BUTTON1_DOWN_MASK);
			
			r.delay(300);
			r.mouseMove(273,56);
			r.mousePress(InputEvent.BUTTON1_DOWN_MASK);
			r.mouseRelease(InputEvent.BUTTON1_DOWN_MASK);			
			
			r.keyPress(17);
			r.keyPress(86);
			r.keyRelease(86);
			r.keyRelease(17);
			
			r.delay(300);
			r.keyPress(10);
			r.keyRelease(10);
			
		}
		catch(Exception e){
			e.printStackTrace();
		}
	}
	
