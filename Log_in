	public void cafe_out() {
		
		try {
			Robot r=new Robot();
			r.delay(300);
			r.mouseMove(469,17);
			r.mousePress(InputEvent.BUTTON1_DOWN_MASK);
			r.mouseRelease(InputEvent.BUTTON1_DOWN_MASK);			
			r.delay(300);			
		}
		catch(Exception e){
			e.printStackTrace();
		}
	}
	
	public void log_out() {
		ClipboardCP cm=new ClipboardCP();
		cm.copy("https://nid.naver.com/nidlogin.logout?returl=https://cafe.naver.com/Logout.nhn?alreadyNaverLogout=true");
		
		try {
			Robot r=new Robot();
			r.delay(300);
			r.mouseMove(273,56);
			r.mousePress(InputEvent.BUTTON1_DOWN_MASK);
			r.mouseRelease(InputEvent.BUTTON1_DOWN_MASK);
			
			r.keyPress(17);
			r.keyPress(86);
			r.keyRelease(86);
			r.keyRelease(17);
			
			r.delay(300);
			r.keyPress(10);
			r.keyRelease(10);
			
			r.delay(600);
			r.mouseMove(471,16);
			r.mousePress(InputEvent.BUTTON1_DOWN_MASK);
			r.mouseRelease(InputEvent.BUTTON1_DOWN_MASK);
			
			r.delay(300);
			r.keyPress(116);
			r.keyRelease(116);
			r.delay(300);
		}
		catch(Exception e){
			e.printStackTrace();
		}
	}
	
	public void cafeClickH() {
		try {
			Robot r=new Robot();			
			
			r.delay(500);
			r.mouseMove(2405,249);
			r.mousePress(InputEvent.BUTTON1_DOWN_MASK);
			r.mouseRelease(InputEvent.BUTTON1_DOWN_MASK);
			
			r.delay(1000);
			r.mouseWheel(1);
			
			r.delay(1000);
			r.mouseMove(736,1012);
			r.mousePress(InputEvent.BUTTON1_DOWN_MASK);
			r.mouseRelease(InputEvent.BUTTON1_DOWN_MASK);
			
			r.delay(1000);
			//r.mouseMove(1150,253);
			r.mouseMove(1139,666);
			r.mousePress(InputEvent.BUTTON1_DOWN_MASK);
			r.mouseRelease(InputEvent.BUTTON1_DOWN_MASK);
		}
		catch(Exception e){
			e.printStackTrace();
		}
	}
	
